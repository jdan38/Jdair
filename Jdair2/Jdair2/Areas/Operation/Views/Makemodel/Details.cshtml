@model Jdair2.Models.Makemodel
@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2 class="text-info">Category Details</h2>
<br />

<form method="post" asp-action="Details">
    <div class="border backgroundWhite">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <div class="form-group row">
            <div class="col-2">
                <label asp-for="Make" class="col-form-label"></label>
            </div>
            <div class="col-5">
                <input asp-for="Make" class="form-control" disabled/>
            </div>
            <span asp-validation-for="Make" class="text-danger"></span>
            <div class="col-2">
                <label asp-for="Model" class="col-form-label"></label>
            </div>
            <div class="col-5">
                <input asp-for="Model" class="form-control" disabled/>
            </div>
            <span asp-validation-for="Model" class="text-danger"></span>
        </div>
        <div class="form-group row">
            <div class="col-5 offset-2">
                <partial name="_DetailsAndBackToListButton" model="Model.Id" />
            </div>
        </div>
    </div>
</form>

